.box VBoxContainer {
	alignment: 1; // center
	separation: 10;
}

.text_class RichTextLabel {
	theme_override_font_sizes/normal_font_size: 30; // normal size
	theme_override_font_sizes/bold_font_size: 30; // bold size
}
